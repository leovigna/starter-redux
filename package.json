{
  "name": "@leovigna/starter-redux",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "author": "Leo Vigna",
  "scripts": {
    "build": "npm-run-all build:starter-redux",
    "build:starter-redux": "cd packages/starter-redux && npm run build",
    "build:starter-redux-docs": "cd packages/starter-redux-docs && npm run build",
    "clean": "npm-run-all -p clean:lib clean:node_modules",
    "clean:lib": "rimraf packages/*/lib packages/*/dist",
    "clean:node_modules": "rimraf node_modules packages/*/node_modules",
    "husky:install": "husky install",
    "test": "npm-run-all -p test:starter-redux",
    "test:starter-redux": "cd packages/starter-redux && npm run test",
    "lint": "cd packages/starter-redux && npm run lint",
    "lint:fix": "cd packages/starter-redux && npm run lint:fix",
    "lint:staged": "cd packages/starter-redux && npm run lint:staged",
    "publish": "pnpm publish -r"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "@changesets/cli": "^2.21.1"
  }
}
