{
    "name": "@leovigna/starter-redux",
    "version": "1.0.1",
    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/leovigna/starter-redux.git"
    },
    "homepage": "https://leovigna.github.io/starter-redux",
    "copyright": "Copyright 2021 Leo Vigna",
    "license": "MIT",
    "type": "commonjs",
    "main": "./dist/starter-redux-lib.cjs.js",
    "module": "./dist/starter-redux-lib.es.js",
    "types": "./types/index.d.ts",
    "exports": {
        ".": {
            "types": "./types/index.d.ts",
            "require": "./dist/starter-redux-lib.cjs.js",
            "import": "./dist/starter-redux-lib.es.js"
        }
    },
    "files": [
        "dist/*",
        "lib/*",
        "src/*",
        "LICENSE",
        "README.md"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "clean": "rimraf lib",
        "test": "cross-env NODE_ENV=test mocha 'lib/cjs/**/*.test.js'",
        "build": "npm-run-all tsc esbuild",
        "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
        "esbuild": "node esbuild.config.mjs",
        "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
        "tsc": "tsc",
        "tsc:watch": "tsc -w",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "lint:staged": "lint-staged",
        "prepublishOnly": "npm run test",
        "publish": "pnpm publish --no-git-checks --access public"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.6.1",
        "axios": "0.25.0",
        "dexie": "^3.2.2",
        "indexeddb-orm": "../../../indexeddb-orm",
        "indexeddbshim": "^10.0.0",
        "lodash": "^4.17.21",
        "re-reselect": "^4.0.0",
        "redux": "^4.1.1",
        "redux-batched-actions": "^0.5.0",
        "redux-orm": "^0.16.2",
        "redux-saga": "^1.1.3",
        "reselect": "^4.0.0",
        "tiny-invariant": "1.2.0",
        "typed-redux-saga": "^1.3.1",
        "uuid": "^8.3.2"
    },
    "peerDependencies": {
        "dexie-react-hooks": "^1.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4"
    },
    "devDependencies": {
        "@esbuild-plugins/node-resolve": "^0.1.4",
        "@redux-saga/is": "^1.1.2",
        "@redux-saga/symbols": "^1.1.2",
        "@testing-library/react": "^12.1.3",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/chai": "^4.2.21",
        "@types/lodash": "^4.14.179",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.4.13",
        "@types/react": "^17.0.40",
        "@types/react-dom": "^17.0.13",
        "@types/react-redux": "^7.1.24",
        "@typescript-eslint/eslint-plugin": "^4.29.0",
        "@typescript-eslint/parser": "^4.29.0",
        "chai": "^4.3.4",
        "cross-env": "^7.0.3",
        "dexie-react-hooks": "^1.1.1",
        "dotenv": "^10.0.0",
        "esbuild": "^0.14.25",
        "esbuild-plugin-alias": "^0.2.1",
        "esbuild-plugin-copy": "^1.1.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^2.5.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "fake-indexeddb": "^3.1.8",
        "glob": "^8.0.3",
        "lint-staged": "^11.1.2",
        "mocha": "^9.0.3",
        "mocha-jsdom": "^2.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.3.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.4",
        "redux-saga-test-plan": "^4.0.3",
        "rimraf": "^3.0.2",
        "typed-redux-saga": "^1.3.1",
        "typescript": "^4.4.3"
    }
}
